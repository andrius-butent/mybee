    <String name="PostIndex" precision="20" label="=postIndex" />
    <Relation name="City" relation="Cities" label="=city" />
    <Relation name="Country" relation="Countries" label="=country" />
    <String name="Note" precision="100" label="=note" />
  </Fields>
</Table>