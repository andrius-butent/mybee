      label="=trRegistrationStatus" />

    <String name="CompanyName" notNull="true" precision="100" label="=trRegistrationCompanyName" />
    <String name="CompanyCode" notNull="true" precision="30" label="=trRegistrationCompanyCode" />
    <String name="VatCode" precision="30" label="=trRegistrationVatCode" />

    <String name="Contact" precision="50" notNull="true" label="=trRegistrationContact" />
