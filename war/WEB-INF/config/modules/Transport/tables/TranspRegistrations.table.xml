    <String name="ExchangeCode" precision="30" label="=trRegistrationExchangeCode" />

    <String name="Bank" precision="100" label="=trRegistrationBank" />
    <String name="BankAddress" precision="100" label="=trRegistrationBankAddress" />
    <String name="BankAccount" precision="30" label="=trRegistrationBankAccount" />
    <String name="Swift" precision="30" label="=trRegistrationSwift" />

    <Text name="Notes" label="=trRegistrationNotes" />
    <Text name="Host" label="=remoteHost" />
    <Text name="Agent" label="=userAgent" />
