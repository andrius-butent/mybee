    <String name="Country" precision="60" notNull="true" label="=trRegistrationCountry" />

    <String name="Phone" precision="50" notNull="true" label="=trRegistrationPhone" />
    <String name="Mobile" precision="50" label="=trRegistrationMobile" />

    <String name="Fax" precision="50" label="=trRegistrationFax" />
