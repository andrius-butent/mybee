<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Table xmlns="http://www.butent.com/table"
  
  name="TranspRegistrations"
  idName="TranspRegistrationID"
  >
  <Fields>
    <DateTime name="Date" notNull="true" label="=trRegistrationDate" defExpr="CURRENT_TIME" />
