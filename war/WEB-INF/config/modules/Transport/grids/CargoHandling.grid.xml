<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column" xmlns:g="http://www.butent.com/grid"
        xmlns:ui="http://www.butent.com/ui"

        name="CargoHandling" viewName="CargoHandling" caption="=cargoHandlingPlaces"
        newRowForm="CargoPlace" newRowCaption="=trNewCargoPlace"
        editForm="CargoPlace"
        flexGrow="1" flexShrink="1"
    >
  <g:body height="50" />

  <g:columns>
    <CalcColumn name="Loading" type="string" cellType="html"
                caption="=loading" minWidth="140" flexBasis="120">
      <style>
        <bodyStyle>
          <ui:inline>overflow-y:auto;</ui:inline>
        </bodyStyle>
      </style>
    </CalcColumn>
    <CalcColumn name="Unloading" type="string" cellType="html"
                caption="=unloading" minWidth="140" flexBasis="120">
      <style>
        <bodyStyle>
          <ui:inline>overflow-y:auto;</ui:inline>
        </bodyStyle>
      </style>
    </CalcColumn>
    <DataColumn name="Notes" editInPlace="true" visible="false" />
  </g:columns>
</g:Grid>
