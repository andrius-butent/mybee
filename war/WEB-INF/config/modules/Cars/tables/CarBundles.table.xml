<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

       name="CarBundles"
       idName="BundleID"
>
  <Fields>
    <String name="BundleName" precision="100" notNull="true" label="=name" />
    <String name="Code" precision="20" unique="true" label="=code" />

    <Relation name="Model" relation="VehicleModels" label="=vehicleModel" />
    <Relation name="Customer" relation="Companies" label="=customer" />

    <Date name="ValidUntil" label="=validUntil" />
    <Text name="Notes" label="=notes" />
  </Fields>
</Table>