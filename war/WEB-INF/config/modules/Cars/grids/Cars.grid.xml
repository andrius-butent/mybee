<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column" xmlns:g="http://www.butent.com/grid"
        xmlns:w="http://www.butent.com/widget"

        name="Cars" viewName="Cars" caption="=cars"
        flexGrow="1" autoFlex="true" cacheDescription="false"
>
  <g:west size="160" splitterSize="2">
    <w:Tree caption="=vehicleModelsShort" />
  </g:west>

  <g:columns>
    <RelColumn name="Type" />
    <DataColumn name="BrandName" />
    <DataColumn name="ModelName" />
    <DataColumn name="BodyNumber" />
    <DataColumn name="Number" />
    <RelColumn name="Owner" />
    <DataColumn name="ProductionDate" caption="=vehicleProductionDateShort" width="80" />
    <RelColumn name="Fuel" />
    <DataColumn name="Power" />
    <DataColumn name="EngineVolume" />
    <DataColumn name="ManufacturerNumber" />
    <DataColumn name="ModelNumber" />
    <RelColumn name="Status" />
    <DataColumn name="ResponsiblePerson" />
    <DataColumn name="Phone" />
    <RelColumn name="CarWarranty" />

    <DataColumn name="EngineNumber" visible="false" />
    <DataColumn name="CylinderNumber" visible="false" />
    <DataColumn name="Horsepower" visible="false" />
    <DataColumn name="DoorsNumber" visible="false" />
    <DataColumn name="GearsNumber" visible="false" />
    <RelColumn name="CarGearbox" visible="false" />
    <RelColumn name="CarPlaceCode" />
    <RelColumn name="CarEmissionStandard" visible="false" />

    <RelColumn name="Item" visible="false" />
  </g:columns>
</g:Grid>
