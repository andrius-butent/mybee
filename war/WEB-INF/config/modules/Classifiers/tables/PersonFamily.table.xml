    <String name="FirstName" precision="30" notNull="true" label="=firstName" />
    <String name="LastName" precision="30" label="=lastName" />
    <Relation name="Relation" relation="FamilyRelationTypes" 
      label="=contactFamilyRelation" />
    <Date name="DateOfBirth" label="=dateOfBirth" />
    <String name="Phone" precision="100" label="=phone" />
    
  </Fields>
</Table>
