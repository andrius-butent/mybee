<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

       name="Files"
       idName="FileID"
       audit="false"
>
  <Fields>
    <String name="Hash" precision="40" notNull="true" unique="true" />
    <Text name="Repository" />
    <String name="Name" precision="255" notNull="true" label="=fileOriginalName" />
    <Long name="Size" label="=fileSize" notNull="true" />
    <String name="Type" precision="100" label="=fileType" />
  </Fields>
</Table>
