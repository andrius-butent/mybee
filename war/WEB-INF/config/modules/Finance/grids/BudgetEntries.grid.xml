<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column" xmlns:g="http://www.butent.com/grid"
        xmlns:ui="http://www.butent.com/ui"
        xmlns:xi="http://www.w3.org/2001/XInclude"

        name="BudgetEntries" viewName="BudgetEntries"
        enabledActions="copy"
>
  <xi:include href="columns/Ordinal.columns.xml" />

  <xi:include href="columns/Indicator.columns.xml" />
  <g:columns>
    <DataColumn name="TurnoverOrBalance" visible="false" />
  </g:columns>

  <xi:include href="columns/BudgetType.columns.xml" />

  <xi:include href="../../Administration/grids/columns/ExtraDimensions.columns.xml" />
  <xi:include href="columns/Employee.columns.xml" />

  <g:columns>
    <DataColumn name="Year" minValue="2000" maxValue="2099" format="0000" />

    <DataColumn name="Month01" scale="0" visible="true" />
    <DataColumn name="Month02" scale="0" visible="true" />
    <DataColumn name="Month03" scale="0" visible="true" />

    <CalcColumn name="Quarter1" type="decimal" format="#,###.#####">
      <style>
        <bodyStyle>
          <ui:font>bolder</ui:font>
        </bodyStyle>
      </style>
    </CalcColumn>

    <DataColumn name="Month04" scale="0" visible="true" />
    <DataColumn name="Month05" scale="0" visible="true" />
    <DataColumn name="Month06" scale="0" visible="true" />

    <CalcColumn name="Quarter2" type="decimal" format="#,###.#####">
      <style>
        <bodyStyle>
          <ui:font>bolder</ui:font>
        </bodyStyle>
      </style>
    </CalcColumn>

    <DataColumn name="Month07" scale="0" visible="true" />
    <DataColumn name="Month08" scale="0" visible="true" />
    <DataColumn name="Month09" scale="0" visible="true" />

    <CalcColumn name="Quarter3" type="decimal" format="#,###.#####">
      <style>
        <bodyStyle>
          <ui:font>bolder</ui:font>
        </bodyStyle>
      </style>
    </CalcColumn>

    <DataColumn name="Month10" scale="0" visible="true" />
    <DataColumn name="Month11" scale="0" visible="true" />
    <DataColumn name="Month12" scale="0" visible="true" />

    <CalcColumn name="Quarter4" type="decimal" format="#,###.#####">
      <style>
        <bodyStyle>
          <ui:font>bolder</ui:font>
        </bodyStyle>
      </style>
    </CalcColumn>

    <CalcColumn name="Total" type="decimal" label="=total" format="#,###.#####">
      <style>
        <bodyStyle>
          <ui:font>bolder larger</ui:font>
        </bodyStyle>
      </style>
    </CalcColumn>

    <DataColumn name="Note" flexGrow="1" visible="false" />
  </g:columns>
</g:Grid>
